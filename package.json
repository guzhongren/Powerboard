{
  "version": "0.0.1",
  "author": "yixi",
  "scripts": {
    "start": "yarn dev",
    "dev": "webpack-dev-server --config build/webpack.dev.js --progress",
    "build": "webpack --config build/webpack.build.js",
    "test": "jest",
    "lint": "yarn tslint --project tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx,json,md}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "dayjs": "^1.8.19",
    "graphql-request": "^1.8.2",
    "lodash": "^4.17.15",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-grid-layout": "^0.18.3",
    "react-router-dom": "^5.1.2",
    "swr": "^0.1.16"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-grid-layout": "^0.17.1",
    "autoprefixer": "^9.7.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
